<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <style>
        body {
            margin: 0;
            padding: 0;
            background-color: #f5f5f5;
        }
    </style>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <div class="container-fluid" style="text-align: center; background-color: #d8deef; margin: 0">
        <img src="https://file.hstatic.net/200000722513/file/banner_topbar_-_b1400ceae_bv3186w_84faf438decf4ff3ac4d29f3a5e3b069.png" alt="Laptop Asus ExpertBook B1400CEAE BV3186W" class="img-fluid" style="width: 79%" />
    </div>
    <div class="container-fluid" style="background-color: #e30019; display: flex">
        <div class="container">
            <div class="row">
                <div class="col-7">
                    <div class="col-3">
                        <img src="https://file.hstatic.net/200000636033/file/logo_fd11946b31524fbe98765f34f3de0628.svg" alt="GEARVN.COM" class="img-fluid" />
                    </div>
                </div>
                <div class="col-5">

                </div>
            </div>
        </div>
    </div>
    @RenderBody()

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>
